{
  "expo": {
    "name": "Caução PIX",
    "slug": "caucao-pix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mce-logo.png",
    "scheme": "caucaopix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/mce-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f68c51"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mce-logo.png",
        "backgroundColor": "#f68c51"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lucianjr.caucaopix"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/mce-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mce-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f68c51",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "30df7a24-7fc4-4355-94d4-bb6a15143880"
      }
    },
    "owner": "lucian-jr"
  }
}
